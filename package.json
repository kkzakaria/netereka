{
  "name": "netereka",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:worker": "npx opennextjs-cloudflare",
    "preview": "npx wrangler dev",
    "deploy": "npm run build:worker && npx wrangler deploy",
    "start": "next start",
    "lint": "eslint",
    "db:migrate": "npx wrangler d1 execute netereka-db --local --file=db/migrations/0001_initial.sql",
    "db:seed": "npx wrangler d1 execute netereka-db --local --file=db/seeds/seed.sql"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@hookform/resolvers": "^5.2.2",
    "@hugeicons/core-free-icons": "^3.1.1",
    "@hugeicons/react": "^1.1.4",
    "@opennextjs/cloudflare": "^1.16.1",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jose": "^6.1.3",
    "nanoid": "^5.1.6",
    "next": "16.1.6",
    "nuqs": "^2.8.7",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260131.0",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "shadcn": "^3.8.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.61.1"
  }
}
